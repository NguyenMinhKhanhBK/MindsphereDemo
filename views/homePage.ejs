        <a class="nav-link border rounded-circle" data-toggle="tab" href="#pumpDetail"><i class="far fa-list-alt"></i></a>
      </li>

    </ul>

    <!-- Tab content -->
    <div class="tab-content">
      <!-- HOME PANEL -->
      <div id="home" class="container-fluid tab-pane active px-5"><br>
        <div class="row px-5 mx-3" style="height:27%">
          <div class="col-sm my-4 ml-4 mr-4 div-dashboard p-2" id="motorEfficiency">
            <label class="text-white dashboard-header">Motor Efficiency</label>

          </div>

          <div class="col-sm  my-4 mx-4 div-dashboard  p-2" id="pumpEfficiency">
            <p class="text-white dashboard-header">Pump Efficiency</p>
          </div>

          <div class="col-sm  my-4 mx-4 div-dashboard  p-2" id="plantAvailability">
            <p class="text-white dashboard-header">Plant Availability</p>
          </div>

          <div class="col-sm  my-4 ml-4 mr-4 div-dashboard  p-2">
            <img src="/images/machine.png" height="95%" style="margin:auto; display: block" alt="">
          </div>

        </div>

        <div class="row div-dashboard mt-2 px-5 text-white"
          style="max-height: 60%; font-size: 30px; padding: 20px ; border-color: #05a993 ; background: #181616 ; overflow: auto ">
          <p style="font-size:35px"><b>Notification List</b></p>
          <table class="table text-white mt-4 ">
            <thead style="color:#05a993">
              <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>John</td>
