                <td>july@example.com</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>

      <!-- PUMP DETAIL PANEL -->
      <div id="pumpDetail" class="container-fluid tab-pane fade"><br>
        <div class="row px-5 mx-3">
          <div class="text-white border px-5 py-1 mx-3" style="font-size:20px">
            E-Pump
          </div>
        </div>

        <div class="row mt-4 mx-4 px-5">
          <div class="col-sm px-4">
            <div class="row text-white" style="border-radius:60px; border-width: 1px !important; border-style: solid !important; border-color: #05a993 ;padding-top: 1%;
                margin: 0px auto;
                float: none;">
              <p class="my-auto px-5 py-auto" style="font-size:35px"><b>Alert / Warning</b></p>

            </div>
            <div class="my-4 mx-auto">
              <img src="/images/machine.png" height="200px" width="200px" style="margin:auto; display: block" alt="">
            </div>
            <div class="container" style="border-radius: 15px !important;
                  border-width: 1px !important;
                  border-style: solid !important;
                  border-color: #05a993 ;padding-top: 1%;
                  margin: 0px auto;
                  float: none;
                  background: #181616">
                  <div class="row px-3">
                      <p style="color:white ; font-size:30px ; margin-left: 20px; display: block">Product specifications</p>
                  </div>
             <div class="row px-3">
                <div class="col-sm mx-4">
                    <div class="row spec-detail my-3">
                      <p class="spec-content mr-auto my-auto">Product type</p>
                      <p class="spec-content my-auto">Centrifugal</p>
                    </div>
    
                    <div class="row spec-detail my-3">
                        <p class="spec-content mr-auto my-auto">Stages</p>
                        <p class="spec-content my-auto">Multistages</p>
                      </div>
    
                      <div class="row spec-detail my-3">
                          <p class="spec-content mr-auto my-auto">Pump Orientation</p>
                          <p class="spec-content my-auto">Vertical</p>
                        </div>
    
                        <div class="row spec-detail my-3">
                            <p class="spec-content mr-auto my-auto">Speed</p>
                            <p class="spec-content my-auto">3500 RPM</p>
                          </div>
    
                          <div class="row spec-detail my-3">
                              <p class="spec-content mr-auto my-auto">Pressure</p>
                              <p class="spec-content my-auto">16 bar</p>
                            </div>
                  </div>
                  <div class="col-sm mx-4">
                      <div class="row spec-detail my-3">
                          <p class="spec-content mr-auto my-auto">Rated Flow</p>
                          <p class="spec-content my-auto">22 m3/s</p>
                        </div>
    
                        <div class="row spec-detail my-3">
                            <p class="spec-content mr-auto my-auto">Rated Head</p>
                            <p class="spec-content my-auto">22.8 m</p>
                          </div>
    
                          <div class="row spec-detail my-3">
                              <p class="spec-content mr-auto my-auto">Pump Consumption</p>
                              <p class="spec-content my-auto">0.37 KW</p>
                            </div>
    
                            <div class="row spec-detail my-3">
                                <p class="spec-content mr-auto my-auto">Operating Pressure</p>
                                <p class="spec-content my-auto">PN 16 bar</p>
                              </div>
    
                              <div class="row spec-detail my-3">
                                  <p class="spec-content mr-auto my-auto">Maximum Pressure</p>
                                  <p class="spec-content my-auto">16 bar</p>
                                </div>
                  </div>
             </div>
              
            </div>  
          </div>
          <div class="col-sm px-4">
            <div class="row  detail-div text-white px-5" style="background:#181616">
              <p style="font-size:35px"><b>Notification List</b></p>
             
            </div>
          </div>
